@model IEnumerable<Dealership.Models.DealershipViewModel.OrderGroup>
@{
    ViewData["Title"] = "Ordered Cars Statistics";
}
<h2>Ordered Books Statistics</h2>
<table>
    <tr>
        <th>
            Order Date
        </th>
        <th>
            Ordered Cars
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderDate)
            </td>
            <td>
                @item.CarCount
            </td>
        </tr>
    }
</table>